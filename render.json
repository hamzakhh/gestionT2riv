{
  "version": 1,
  "name": "gestiont2riv-tunisian",
  "type": "static",
  "build": {
    "command": "npm install && npm run build",
    "publish": "dist"
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
